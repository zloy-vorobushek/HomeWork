using System;

namespace HomeWork
{
    class Program
    {
        static void Main()
        {
            int totalImages = 52;
            int outputOfImages = 3;
            int filledRows = totalImages / outputOfImages;
            int picturesBeyondMeasure = 52 - filledRows * outputOfImages;
            Console.WriteLine($"{filledRows} полностью заполненных рядов и {picturesBeyondMeasure} картинок будетсверх меры.");
        }
    }
}
