using System;

namespace HomeWork
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine("Введите количество повторений.");
            int replayMessageCount = int.Parse(Console.ReadLine());
            for (int i = 0; i < replayMessageCount; i++)
            {
                Console.WriteLine("Hello, World!");
            }
        }
    }
}
